{
  "name": "Quba",
  "version": "1.1.0",
  "description": "Viewer",
  "author": "Quba",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && electron-builder build --win --publish never",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "pack": "npm run build && electron-builder build --publish=never",
    "start": "vue-cli-service build && electron .",
    "dist": "electron-builder",
    "publish": "electron-builder build --win --publish always"
  },
  "main": "app/main.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "custom-electron-titlebar": "^3.2.7",
    "electron-localshortcut": "^3.2.1",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.9",
    "fast-xml-parser": "^3.19.0",
    "i18next": "^20.6.0",
    "i18next-node-fs-backend": "^2.1.3",
    "pdfjs-dist": "^2.9.359",
    "saxon-js": "^2.3.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.1.7",
    "vue-tabs-chrome": "^0.10.0",
    "vue3-tabs-chrome": "^0.2.1"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^3.0.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "electron": "^12.0.0",
    "electron-builder": "^22.11.7",
    "vue-cli-plugin-i18n": "~2.1.3"
  },
  "build": {
    "appId": "org.quba-viewer.viewer",
    "mac": {
      "category": "Document",
      "icon": "./assets/img/logo_small_icon_only.png"
    },
    "win": {
      "icon": "./assets/img/logo_small_icon_only.png"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "**/*",
      "dist/**/*",
      "!*.map",
      "!package.json",
      "!package-lock.json"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "ZUGFeRD",
        "repo": "quba-viewer"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asimkhanaup/quba-viewer.git"
  },
  "keywords": [],
  "license": "APL"
  
}
